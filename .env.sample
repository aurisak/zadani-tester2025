# Example .env file for Faktura Online E2E Tests
# Copy this file to .env and update with your own credentials

# Domain selection for tests (cz, com, or sk)
TEST_DOMAIN=cz

# CZ domain credentials
CZ_EMAIL=your.email@example.com
CZ_PASSWORD=your_password_here

# COM domain credentials
COM_EMAIL=your.english@example.com
COM_PASSWORD=your_com_password_here

# SK domain credentials
SK_EMAIL=your.slovak@example.com
SK_PASSWORD=your_sk_password_here
